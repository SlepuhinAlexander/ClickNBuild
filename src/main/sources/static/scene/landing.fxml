<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Text?>
<StackPane fx:id="rootNode" prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/10.0.2-internal"
           xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.itworks.clicknbuild.ui.controller.LandingController">
    <ImageView fx:id="icLandingBackground" fitHeight="${rootNode.height}" preserveRatio="true"/>
    <Text fx:id="sLandingGreeting" styleClass="landing__greeting" StackPane.alignment="CENTER"/>
</StackPane>
